@model Lykke.Frontend.WampHost.Models.MethodInfoModel
<html>
<head>
    <title>Wamp documentation</title>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/highlight.js/9.12.0/styles/default.min.css">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" asp-append-version="true" />
</head>
<body>
<div class="alert alert-info alert-link">
    <i class="glyphicon glyphicon-info-sign"></i> Swagger documentation can be found <a href="/swagger/ui">here</a>
    <i class="glyphicon glyphicon-info-sign"></i> Source code and readme can be found <a href="https://github.com/LykkeCity/Lykke.Frontend.WampHost">here</a>
</div>
<div role="tabpanel">
    <ul class="nav nav-tabs" role="tablist">        
        <li role="presentation" class="active">
            <a href="#topics" aria-controls="topics" role="tab" data-toggle="tab">topics</a>
        </li>
    </ul>
    <div class="tab-content">        
        <div role="tabpanel" class="tab-pane active" id="topics">
            @Html.Partial("MethodsListPartial", Model.Topic)
        </div>
    </div>
</div>
    
<script src="https://code.jquery.com/jquery.min.js" asp-append-version="true"></script>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/highlight.js/9.12.0/highlight.min.js"  asp-append-version="true"></script>
<script>hljs.initHighlightingOnLoad();</script>

</body>
</html>